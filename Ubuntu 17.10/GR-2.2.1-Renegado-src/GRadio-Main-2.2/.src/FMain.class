Public Sub ButtonUP_Click()
Dim ruta As String
Dim ruta1 As String
  Dim anterior As String

  If ListBoxParrillaRaiz.Index = 0 Then
    Message.Error("No se puede subir más")

  Else If ListBoxParrillaRaiz.count = 0 Then
    Message.Error("La lista de elementos está vacía")

  Else If ListBoxParrillaRaiz.Index = -1 Then
    Message.Error("Primero selecciona un elemento")

  Else
    anterior = ListBoxParrillaRaiz[ListBoxParrillaRaiz.Index - 1].Text
    ListBoxParrillaRaiz[ListBoxParrillaRaiz.Index - 1].Text = ListBoxParrillaRaiz[ListBoxParrillaRaiz.Index].Text
    ListBoxParrillaRaiz[ListBoxParrillaRaiz.Index].Text = anterior
    ListBoxParrillaRaiz.Index = ListBoxParrillaRaiz.Index - 1
    ruta = User.home & "/.gradio/data/tmp/playlist"
ruta1 = User.home & "/.gradio/data/tmp/playlistg3"
File.Save(ruta, ListBoxParrillaRaiz.List.Join("\n") & "\n")
Try File.Save(ruta1, ListBoxParrillaRaiz.List.Join("\n") & "\n")
Shell "~/.gradio/bin/cartucho.sh"

  Endif
  
  

End

Public Sub ButtonDown_Click()
Dim ruta As String
Dim ruta1 As String
  Dim siguiente As String

  If ListBoxParrillaRaiz.Index = ListBoxParrillaRaiz.Count - 1 Then
    Message.Error("No se puede bajar más")

  Else If ListBoxParrillaRaiz.count = 0 Then
    Message.Error("La lista de elementos está vacía")

  Else If ListBoxParrillaRaiz.Index = -1 Then
    Message.Error("Primero selecciona un elemento")

  Else
    siguiente = ListBoxParrillaRaiz[ListBoxParrillaRaiz.Index + 1].Text
    ListBoxParrillaRaiz[ListBoxParrillaRaiz.Index + 1].Text = ListBoxParrillaRaiz[ListBoxParrillaRaiz.Index].Text
    ListBoxParrillaRaiz[ListBoxParrillaRaiz.Index].Text = siguiente
    ListBoxParrillaRaiz.Index = ListBoxParrillaRaiz.Index + 1
    ruta = User.home & "/.gradio/data/tmp/playlist"
ruta1 = User.home & "/.gradio/data/tmp/playlistg3"
File.Save(ruta, ListBoxParrillaRaiz.List.Join("\n") & "\n")
Try File.Save(ruta1, ListBoxParrillaRaiz.List.Join("\n") & "\n")
Shell "~/.gradio/bin/cartucho.sh"

  Endif

End

