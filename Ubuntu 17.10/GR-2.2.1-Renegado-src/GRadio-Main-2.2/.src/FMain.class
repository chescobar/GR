Private Sub cargaBuscar(ruta As String, l As ListBox)

  Dim linea As New String[]
  Dim a As Integer

  
  Try linea = Split(File.Load(ruta), "\n")
 'If Error Then Return
  l.Clear()
  For a = 0 To linea.count - 2
    l.Add(linea[a])
  Next
End

