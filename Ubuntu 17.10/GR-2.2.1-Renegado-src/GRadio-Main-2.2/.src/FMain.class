''Vumeter
Public Sub btnStart_Click()
Dim phandle As Pointer
Dim sdevice As String = "default"
Dim err, fc, iVol, i As Integer
Dim ichannel As Byte
Dim abuffer As New Short[iSize]

   ' Einstellen der Frequenz auf 44.1 kHz
   fc = 44100

   ' Festlegen der Kanalanzahl (1=mono, 2=stereo)
   ichannel = 2

   ' Registrieren von ALSA zur Aufnahme
   ' err = snd_pcm_open(VarPtr(phandle), sdevice, SND_PCM_STREAM_CAPTURE, 0)
   err = snd_pcm_open(VarPtr(phandle), sdevice, SND_PCM_STREAM_PLAYBACK, 0)
   
   
   If err < 0 Then Error.Raise("Fehler beim Öffnen des PCM Subsystems: " & snd_strerror(err))

   ' Einstellen der ALSA PCM Parameter
   err = snd_pcm_set_params(phandle, SND_PCM_FORMAT_S16_LE, SND_PCM_ACCESS_RW_INTERLEAVED, ichannel, fc, 1, 0)
   If err < 0 Then Error.Raise("Fehler beim Einstellen der PCM Parameter: " & snd_strerror(err))

   $bStop = True

   While $bStop   ' läuft bis 'Stop' gedrückt wird  

   ' Übernahme der Daten in das abuffer-Array
   err = snd_pcm_readi(phandle, abuffer, iSize / 2)
   If err < 0 Then Error.Raise("Fehler bei der Audiodatenaufzeichnung " & snd_strerror(err))

   ' Berechnung der Summe der Volume-Werte 
   For i = 0 To iSize - 1
      iVol += Abs(abuffer[i])
   Next
  
   hVol = iVol 
   PrepareDraw()
   iVol = 0

   ' 1 ms Warten, um Programmeingaben vornehmen zu können
   Wait 0.001
   Wend
   
   If $bStop = False Then snd_pcm_close(phandle)
   abuffer.Clear()

End



Public Sub btnStop_Click()
  $bStop = False
End

Public Sub PrepareDraw()
Dim aColors As Integer[] = [Color.Blue, Color.Green, Color.Yellow, Color.Orange, Color.red] ' Farbskale für Volume-Balken
Dim aPositions As Float[] = [0, 0.1, 0.15, 0.17, 0.23]                                  ' Farbverlaufspunkte
   
   $hPic = New Picture(drwVol.Width, drwVol.Height, True)
   
   Paint.Begin($hPic)
      Paint.Translate(3, drwVol.H)                                            ' Um 3 nach rechts und Höhe der Zeichenfläche verschieben
      Paint.Scale(1, -5)                                                      ' y nach oben
         
      Paint.Brush = Paint.LinearGradient(drwVol.w / 2, 0, drwVol.w / 2, drwVol.H, aColors, aPositions)
      Paint.Rectangle(0, 0, 45, drwVol.H)                                     ' Farbverlaufsskala malen
      Paint.Fill()
   
      hVol = Abs(hVol / 2 ^ 15 / iSize * drwVol.H - 5)                        ' Max. Volumenwert ist 5 kleiner als drwVol.H
      Paint.Brush = Paint.Color(Color.Black)                       ' Einen Pinsel in Hintergrundfarbe
      Paint.Rectangle(0, hvol, drwVol.W, drwVol.H)                            ' Rechteck, das nur den anzuzeigenden Bereich des Volumenbalkens freigibt und den Rest überdeckt 
      Paint.Fill() 
   Paint.End
   drwVol.Refresh()
End


Public Sub drwVol_Draw()  
  Paint.Begin(drwVol)
    If $hPic Then Paint.DrawPicture($hPic, 0, 0)  
  Paint.End
End 
''Vumeter-end




